{
    "terminals": [
        {
            "class": "Alacritty",
            "command": "alacritty"
        },
        {
            "class": "kitty",
            "command": "kitty"
        }
    ],
    "subprocesses": [
        {
            "name": "cmus",
            "launch_command": "{command} && i3-msg kill"
        },
        {
            "name": "codex"
        },
        {
            "name": "ipython"
        },
        {
            "name": "man"
        },
        {
            "name": "nvim",
            "exclude_args": ["--embed"]
        },
        {
            "name": "pipenv",
            "include_args": ["shell"]
        },
        {
            "name": "ssh"
        },
        {
            "name": "su"
        },
        {
            "name": "sudo",
            "include_args": ["-i", "--login"]
        },
        {
            "name": "vim"
        }
    ],
    "web_browsers": [
        "firefox"
    ],
    "enabled_plugins": {}
}
